## The function is currently defined as
function (data, correct = TRUE, ...) 
{
    N <- nrow(data)
    Tint <- intWC(data, correct = correct, ...)
    Lint <- numeric(N)
    for (i in 1:N) Lint[i] <- intWC(data[-i, ], ...)
    Dint <- N * Tint - (N - 1) * Lint
    Dint
  }
}
