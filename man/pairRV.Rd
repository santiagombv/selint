function (mat, vars1, vars2) 
{
    S12 <- mat[vars1, vars2]
    S11 <- mat[vars1, vars1]
    S22 <- mat[vars2, vars2]
    RV <- sum(S12^2)/sqrt(sum(S11^2) * sum(S22^2))
    RV
  }
}
